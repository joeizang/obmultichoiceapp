    "react-hook-form": "^6.9.4",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "@types/react": "^16.9.51",
    "@types/react-bootstrap": "^0.32.24",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-bootstrap": "^0.24.5",
    "@types/react-router-dom": "^5.1.6",
    "ajv": "^6.9.1",
    "cross-env": "^5.2.0",
    "eslint": "^6.8.0",
